<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="gov:usda:aphis:pcit:PCIT_ShipmentCode" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:simpleType name="CommercialCode">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Y">
				<xs:annotation>
					<xs:documentation>Is Commercial - Shipment value greater than or equal to 1250</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="N">
				<xs:annotation>
					<xs:documentation>Is Commercial - Shipment value less than 1250</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
